  nb_prem: '$0.00', nb_pol: 0,
  rn_prem: '$0.00', rn_pol: 0,
  rw_prem: '$0.00', rw_pol: 0,
  tot_prem: '$0.00', tot_pol: 0
};

 // Las variables panelToday, panelMonth, panelCompanyToday, panelCompanyMonth, location_type y agency_name
 // vienen SIEMPRE desde el backend. Si por alguna razón alguna falta, usamos defaultPanel.
const pToday = panelToday || defaultPanel;
const pMonth = panelMonth || defaultPanel;
const pCompanyToday = panelCompanyToday || defaultPanel;
const pCompanyMonth = panelCompanyMonth || defaultPanel;

const panelTodayText = location_type == 1 ? "Corporate - Today" : (agency_name || "Agency") + " - Today";
const panelMonthText = location_type == 1 ? "Corporate - This Month" : (agency_name || "Agency") + " - This Month";
%>

